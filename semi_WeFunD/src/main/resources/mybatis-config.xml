        <mapper resource="/mappers/adminMappers/project-mapper.xml" />
        <mapper resource="/mappers/adminMappers/review-mapper.xml" />
        <mapper resource="/mappers/adminMappers/sales-mapper.xml" />
        <mapper resource="/mappers/adminMappers/member-mapper.xml" />	
		<mapper resource="/mappers/category-mapper.xml" />
		<mapper resource="/mappers/main-mapper.xml" />
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/myPage-mapper.xml" />
		<mapper resource="/mappers/notice-mapper.xml" />
		<mapper resource="/mappers/project-mapper.xml" />
		<mapper resource="/mappers/qna-mapper.xml" />
		<mapper resource="/mappers/review-mapper.xml" />
	</mappers>
	 <!-- 유의사항 : 미리 등록해두려면 해당 mapper파일이 진짜 존재하고 있어야함!!!(namespace까지!!) -->
</configuration>
